        "headers": {
            "X-Grab-Device-ID": "{random_device_id}"
        },
        "data": {
            "phoneNumber": "{phone}",
            "countryCode": "ID",
            "method": "CALL"
        }
    },
    {
        "name": "[SMS] Eraspace",
        "url": "https://api.eraspace.com/customer/v1/otp",
        "method": "POST",
        "provider": "all",
        "data": {
            "phone": "{c}",
            "otpType": "REGISTER"
        }
    },
    {
        "name": "[SMS] Marugame",
        "url": "https://api.marugame.co.id/api/v1/auth/otp",
